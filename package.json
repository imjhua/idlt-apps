{
  "name": "idlt-apps",
  "private": true,
  "version": "1.0.0",
  "repository": "https://github.com/imjhua/idlt-apps.git",
  "type": "module",
  "scripts": {
    "push": "git checkout dev && git add . && git commit -m 'add' && git push",
    "merge": "git checkout main && git merge dev && git push",
    "git": "yarn build && yarn push && yarn merge",
    "========================================== [ENVIRONMENT]": "",
    "env": "cp .env/.env.development .env.local",
    "env:prod": "cp .env/.env.production .env.local",
    "========================================== [SERVER]": "",
    "dev": "yarn env && vite --port 2222 --open http://idlt.localhost:2222 --force --debug",
    "start": "vite build && vite preview --port 2222 --open http://idlt.localhost:2222",
    "build": "yarn env:prod && yarn lint && tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/eslint-plugin": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@types/node": "^20.6.5",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "axios": "^1.4.0",
    "eslint": "^8.44.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "firebase": "^10.4.0",
    "grommet": "^2.33.2",
    "grommet-icons": "^4.11.0",
    "kakao.maps.d.ts": "^0.1.39",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.14.1",
    "styled-components": "^5.3.11"
  },
  "devDependencies": {
    "@storybook/react": "^7.4.5",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-autofix": "^1.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "5.1.6",
    "vite": "^4.4.5",
    "vite-plugin-babel-macros": "^1.0.6",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-rewrite-all": "^1.0.1",
    "vite-tsconfig-paths": "^4.2.0"
  }
}
